char BiggestBigSmall(char str[])
{
	char c = ' ';
	int i = -1, j=-1;
	while (str[++i] != '\0')
	{
		if (str[i] < 'A' || str[i] > 'Z')
		{
			if ((str[i] >= 'a' && str[i] <= 'z') || str[i] == ' ')
				continue;
			printf("Wong input no %d\n", i);
			continue;
		}
		if (c > str[i])
			continue;
		while (str[++j] != '\0')
			if (str[i] == (str[j] - ' '))
			{
				c = str[i];
				break;
			}
		j = -1;
	}
	if (c == ' ')
		printf("Not found");
	return c;
}
